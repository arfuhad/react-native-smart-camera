{
  "extends": "expo-module-scripts/tsconfig.module",
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true
  },
  "include": ["./src"],
  "exclude": ["node_modules", "build", "example"]
}
